{
  "name": "mongoose-update-if-current",
  "version": "1.0.2",
  "description": "Optimistic concurrency control (OCC) plugin for mongoose",
  "main": "dist/index.js",
  "typings": "typings/index.d.ts",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run tsc",
    "test": "jest",
    "clean": "del-cli 'dist/**' '!dist' 'typings/**' '!typings'",
    "tsc": "tsc --project tsconfig.json",
    "ci": "npm run build && npm test -- --coverage --no-cache",
    "prepack": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eoin-obrien/mongoose-update-if-current.git"
  },
  "keywords": [
    "mongodb",
    "mongoose",
    "plugin",
    "save",
    "concurrency",
    "concurrency control",
    "optimistic concurrency control",
    "versioning"
  ],
  "author": {
    "name": "Eoin O'Brien",
    "email": "eoinobrien910@gmail.com",
    "url": "https://videtur.io/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eoin-obrien/mongoose-update-if-current/issues"
  },
  "homepage": "https://github.com/eoin-obrien/mongoose-update-if-current",
  "devDependencies": {
    "@types/jest": "^22.0.0",
    "@types/mongoose": "^5.0.12",
    "@types/node": "^10.0.4",
    "del-cli": "^1.1.0",
    "jest": "^22.0.4",
    "mongoose": ">=4.8.0",
    "ts-jest": "^22.0.1",
    "ts-node": "^6.0.3",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "typescript": "^2.6.2"
  },
  "dependencies": {},
  "peerDependencies": {
    "mongoose": ">=4.8.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/__tests__/",
      "/node_modules/"
    ],
    "testEnvironment": "node",
    "testRegex": "__tests__/.*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
